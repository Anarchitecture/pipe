{
  "name": "anarchitecture/pipe",
  "type": "library",
  "description": "Unary callable helpers to leverage PHP 8.5 pipes",
  "license": "MIT",
  "require": {
    "php": "^8.5"
  },
  "autoload": {
    "files": [
      "src/pipe.php"
      ]
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "analyse": "phpstan analyse -l 10 src",
    "check": [
      "@analyse"
    ]
  }
}
